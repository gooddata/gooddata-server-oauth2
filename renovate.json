{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>whitesource/merge-confidence:beta", "config:base"],
  "ignorePresets": [":dependencyDashboard"],
  "commitMessagePrefix": "NAS-3753: ",
  "packageRules": [
    {
      "matchPackagePrefixes": ["io.gitlab.arturbosch.detekt"],
      "groupName": "detekt"
    },
    {
      "matchPackagePrefixes": ["org.jetbrains.kotlin"],
      "groupName": "kotlin"
    }
  ]
}
